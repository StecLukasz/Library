{
  "name": "webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check --port 4200",
    "build": "cross-env NODE_ENV=production ng build",
    "build-prod": "npm run lint && npm run build -- --no-progress --base-href=/books/",
    "build-demo": "npm run lint && npm run build -- --no-progress --base-href=/books/ --configuration development",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^14.0.6",
    "@angular/common": "^15.1.0",
